{
    "name": "alireza-h/laravel-devtools",
    "description": ":laravel devtools",
    "authors": [
        {
            "name": "Alireza Hoseinzadeh",
            "email": "alireza.hoseinzadeh89@gmail.com",
            "homepage": "https://github.com/alireza-h"
        }
    ],
    "homepage": "https://github.com/alireza-h/laravel-devtools",
    "keywords": ["Laravel", "LaravelDevTools"],
    "require": {
        "php": ">=7.0",
        "illuminate/cookie": "^7|^8",
        "illuminate/database": "^7|^8",
        "illuminate/events": "^7|^8",
        "illuminate/routing": "^7|^8",
        "illuminate/session": "^7|^8",
        "illuminate/support": "^7|^8",
        "predis/predis": "^1.0",
        "facade/ignition": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "AlirezaH\\LaravelDevTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlirezaH\\LaravelDevTools\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
              "AlirezaH\\LaravelDevTools\\DevToolsServiceProvider"
            ],
            "aliases": {
                "DevTools": "AlirezaH\\LaravelDevTools\\Facades\\DevTools"
            }
        }
    }
}
